<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>7 Wonders {{ page_title|default('') }}</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap.min.css') }}" />
    <!-- Font Awesome -->
    <link rel="stylesheet" href="{{ asset('font-awesome/css/font-awesome.min.css') }}" />
    <!-- Ionicons -->
    <link rel="stylesheet" href="{{ asset('ionicons/css/ionicons.min.css') }}" />
    <link rel="stylesheet" href="{{ asset('datatable/css/dataTables.bootstrap.min.css') }}"/>
    <link rel="stylesheet" href="{{ asset('jquery-ui/css/jquery-ui.css') }}" />
    <link rel="stylesheet" href="{{ asset('jquery-ui/css/theme.css') }}" />
    <link href="{{ asset('select2/css/select2.css') }}" rel="stylesheet" type="text/css" />
    <!-- Theme style -->
    <link rel="stylesheet" href="{{ asset('css/AdminLTE.min.css') }}" />
    <!-- AdminLTE Skins. Choose a skin from the css/skins -->
    <link rel="stylesheet" href="{{ asset('css/skins/_all-skins.min.css') }}" />
    <link rel="stylesheet" href="{{ asset('css/main.css') }}" />

    <!-- Google Font -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
    {% block head %}{% endblock %}
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="#" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>7</b>W</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>7</b>Wonders</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
            </nav>
        </header>
        <!-- Control Sidebar -->
        {% include 'misc/nav.html.twig' with {'session': session, 'nav_menu': nav_menu} %}
        <div class="content-wrapper">
            {{ flash_messages|raw }}
            <section class="content">
                {% block content %}{% endblock %}
            </section>
        </div>
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                Frontend powered by <a href="https://adminlte.io/">AdminLTE</a>
            </div>
            <strong>Copyright &copy; 2017-2018 Marius Strajeru</strong>
        </footer>
    </div>
    <!-- jQuery 3 -->
    <script type="text/javascript" src="{{ asset('jquery/jquery.min.js') }}"></script>
    <!-- jQuery UI 1.11.4 -->
    <script type="text/javascript" src="{{ asset('jquery-ui/js/jquery-ui.min.js') }}"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script type="text/javascript">
        jQuery.widget.bridge('uibutton', jQuery.ui.button);
    </script>
    <!-- Bootstrap 3.3.7 -->
    <script type="text/javascript" href="{{ asset('bootstrap/js/bootstrap.min.js') }}"></script>
    <!-- Slimscroll -->
    <script type="text/javascript" src="{{ asset('slimscroll/jquery.slimscroll.min.js') }}"></script>
    <!-- FastClick -->
    <script type="text/javascript" src="{{ asset('fastclick/fastclick.js') }}"></script>
    <!-- AdminLTE App -->
    <script type="text/javascript" src="{{ asset('select2/js/select2.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/adminlte.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('datatable/js/jquery.dataTables.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('datatable/js/dataTables.bootstrap.js') }}"></script>
    {% block footer %}{% endblock %}
</body>
</html>
